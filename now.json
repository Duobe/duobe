{
  "version": 2,
  "name": "duobe",
  "regions": ["hnd", "sfo","bru"],
  "alias": ["fennie.now.sh"],
  "builds": [
    {
      "use": "@now/static-build",
      "src": "package.json",
      "config": { "distDir": "public" }
    }
  ],
  "routes": [
    {
      "src": "^/public/static/(.*)",
      "headers": { "cache-control": "public,max-age=31536000,immutable" }
    },
    {
      "src": "^/(.*).(css|js)",
      "headers": { "cache-control": "public,max-age=31536000,immutable" }
    },
    {
      "src": "^/(.*).html",
      "headers": { "cache-control": "public,max-age=0,must-revalidate" }
    }
  ]
}
