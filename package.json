{
  "name": "duobe.io",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Duobe <beduo@qq.com>",
  "dependencies": {
    "gatsby": "^2.2.5",
    "gatsby-image": "^2.0.34",
    "gatsby-plugin-manifest": "^2.0.24",
    "gatsby-plugin-offline": "^2.0.25",
    "gatsby-plugin-react-helmet": "^3.0.10",
    "gatsby-plugin-sharp": "^2.0.30",
    "gatsby-source-filesystem": "^2.0.27",
    "gatsby-transformer-sharp": "^2.1.17",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-helmet": "^5.2.0"
  },
  "devDependencies": {
    "prettier": "^1.16.4"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx --ignore-pattern public .",
    "serve": "gatsby serve",
    "develop": "gatsby develop",
    "build": "gatsby build",
    "rebuild": "rm -rf public && rm -rf .cache && npm run build",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "now-build": "npm run build",
    "deploy": "yarn build && cp now.json public/ && cd public",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Duobe/duobe.io.git"
  },
  "bugs": {
    "url": "https://github.com/Duobe/duobe.io/issues"
  }
}
